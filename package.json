{
  "name": "nest-template",
  "version": "0.0.1",
  "description": "",
  "author": "CerradoTech",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start:dev": "nodemon",
    "start:prod": "cross-env NODE_ENV=prod nest start",
    "start:build:dev": "cross-env NODE_ENV=dev node dist/main",
    "start:build:prod": "cross-env NODE_ENV=prod node dist/main",
    "lint:check": "tsc --noEmit && eslint \"{src,apps,libs,test}/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "tsc --noEmit && eslint \"{src,apps,libs,test}/**/*.{js,jsx,ts,tsx}\" --fix",
    "pretty:check": "prettier --config ./.prettierrc.js --check \"{src,apps,libs,test}/**/*.{js,jsx,ts,tsx,json}\"",
    "pretty:write": "prettier --config ./.prettierrc.js --write \"{src,apps,libs,test}/**/*.{js,jsx,ts,tsx,json}\"",
    "postinstall": "tsc",
    "docker:compose": "docker compose -f docker-compose.yml up -d --build"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-i18n": "^1.1.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "module-alias": "^2.2.3",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "stripe": "^13.5.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^5.59.11",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@1.22.19"
}
